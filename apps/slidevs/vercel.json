{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm nx build slidevs",
  "installCommand": "pnpm install",
  "outputDirectory": "dist/2025-06-29",
  "framework": null,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1/index.html"
    }
  ]
}
