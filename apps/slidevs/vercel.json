{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "cd apps/slidevs && pnpm build",
  "installCommand": "pnpm install",
  "outputDirectory": "apps/slidevs/dist",
  "framework": null,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1/index.html"
    }
  ]
}
