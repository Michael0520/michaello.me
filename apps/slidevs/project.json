{
  "name": "slidevs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/slidevs",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "slidev 2025-06-29/src/slides.md",
        "cwd": "apps/slidevs"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "slidev build 2025-06-29/src/slides.md --out dist/2025-06-29",
        "cwd": "apps/slidevs"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "export": {
      "executor": "nx:run-commands",
      "options": {
        "command": "slidev export 2025-06-29/src/slides.md --output ../../dist/2025-06-29.pdf",
        "cwd": "apps/slidevs"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vue-tsc --noEmit",
        "cwd": "apps/slidevs"
      }
    }
  },
  "tags": ["type:app", "platform:slidev"]
}
